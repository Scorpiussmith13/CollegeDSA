// Online C compiler to run C program online
#include <stdio.h>
int* maxAmongUS(int * arr , int n){
    int max = 0 ;
    int *index;
    for(int i=0; i < n ; i++){
        if(*(arr+i) > max) {
            max = *(arr + i);       
            index = (arr+i);         
            
        }
            
        }

    return index;
}
int main() {
    int arr[5] ={100,2,12,400,10};
    int n = sizeof(arr)/sizeof(arr[0]);
   int * index=maxAmongUS(arr , n);  
    printf("%d",*index);
    return 0;
}
